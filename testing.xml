<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Automation Suite">
    <listeners>
        <listener class-name="com.selenium.utility.TestListener"/>
    </listeners>
    <test name="Tests">
        <packages>
            <package name="com.selenium.tests"/>
        </packages>
        <classes>
            <class name="com.selenium.tests.TravelIns"></class>
            <class name="com.selenium.tests.HealthIns"></class>
        </classes>
    </test>
</suite>
